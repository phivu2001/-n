@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@model DTO.Customer.CustomerDTO
<div class="account-management">
    @Html.Partial("_breadcrumb", new Dictionary<string, string>
        { { "Home", "Home" }, { "CustomerManagement", "Customer Management" }, { "C", "Add new" } }, new ViewDataDictionary(ViewData) { { "mHref", Url.Action("Index", "CustomerManagement") }, { "mShowButton", "false" } })

        @using (Html.BeginForm("New", "CustomerManagement", FormMethod.Post, new { @id = "form-create", @class = "form-horizontal needs-validation", @enctype = "multipart/form-data" }))
        {
            @Html.Partial("_Form", Model)
        }
</div>

