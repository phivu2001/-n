@{
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}
@model DTO.Customer.CustomerDTO
<div class="account-management">
    @Html.Partial("_breadcrumb", new Dictionary<string, string>
        { { "Home", "Home" }, { "CustomerManagement", "Users Management" }, { "C", "Edit" } }, new ViewDataDictionary(ViewData) { { "mHref", Url.Action("Index", "CustomerManagement", new { area = "Admin" }) }, { "mShowButton", "false" } })

    @using (Html.BeginForm("Edit", "CustomerManagement", FormMethod.Post, new { @id = "form-create", @class = "form-horizontal needs-validation", @enctype = "multipart/form-data" }))
    {
        @Html.Partial("_Form", Model)
    }
</div>
